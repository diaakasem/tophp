{
	"bin": {
		"tophp": "./src/tophp.js"
	},
	"name": "tophp",
	"version": "1.2.3",
	"description": "Converts CSV or JSON file to PHP array",
	"main": "test.js",
	"scripts": {
		"prettier": "prettier --write '**/*.js'",
		"start": "node tophp.js",
		"test-csv": "./src/tophp.js --csv=./sample.csv",
		"test-json": "./src/tophp.js --json=./sample.json",
		"test": "jest",
		"patch": "npm version patch",
		"prepare": "husky install"
	},
	"keywords": [
		"csv",
		"json",
		"array",
		"php"
	],
	"contributors": [
		{
			"name": "Diaa Kasem",
			"email": "me@diaa.me",
			"url": "https://diaa.me"
		}
	],
	"license": "ISC",
	"dependencies": {
		"@prettier/plugin-php": "^0.18.8",
		"csv-parse": "^5.2.0",
		"fast-csv": "^4.3.6",
		"lodash": "^4.17.21",
		"prettier": "^2.7.1",
		"yargs": "^17.5.1"
	},
	"devDependencies": {
		"git-hooks": "^1.1.10",
		"husky": "^8.0.0",
		"jest": "^28.1.1",
		"jest-cli": "^28.1.1",
		"pretty-format": "^28.1.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"prettier": {
		"trailingComma": "es5",
		"singleQuote": true,
		"printWidth": 120,
		"tabWidth": 4,
		"useTabs": true,
		"semi": true,
		"arrowParens": "always",
		"bracketSpacing": true,
		"endOfLine": "lf"
	}
}
